{
  "exclude": [
    "LICENSE"
  ],
  "fmt": {
    "indentWidth": 2,
    "lineWidth": 120,
    "proseWrap": "never",
    "semiColons": true,
    "singleQuote": false,
    "useTabs": false
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.11"
  },
  "workspace": ["./number", "./string"],
  "publish": {
    "exclude": [
      "LICENSE",
      "README.md",
      "CHANGELOG.md",
      "deno.lock",
      "tests/**"
    ]
  }
}
